{
  "name": "Schoology+",
  "version": "1.0",
  "description": "Adds buttons to events on schoology",
  "manifest_version": 2,
  "content_scripts": [
	{
	    "matches": ["https://bca.schoology.com/*"],
	    "js": ["script.js"],
        "run_at": "document_end"
        //"all_frames": true
	}
  ],
  "browser_action": {
	"default_title": "Adds buttons to events on schoology"
  },
  "permissions": [ "activeTab", "https://ajax.googleapis.com/"]
}